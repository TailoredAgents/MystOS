{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "moduleResolution": "node16",
    "lib": ["ES2021", "DOM", "DOM.Iterable"],
    "types": ["node", "@playwright/test"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowJs": false,
    "baseUrl": "."
  },
  "include": ["./**/*.ts"]
}
